<Page x:Class="bdInterfaces.Pages.Consultas"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
      xmlns:fo="http://schemas.awesome.incremented/wpf/xaml/fontawesome.sharp"

      xmlns:local="clr-namespace:bdInterfaces.Pages"
      mc:Ignorable="d"
      Background="{StaticResource Negro}"
      Title="Consultas" Height="450" Width="600">
    <StackPanel Orientation="Horizontal"
                HorizontalAlignment="Center">
        <ListBox x:Name="lista1"
                 Width="275"
                 Height="250"
                 Background="Transparent"
                 Foreground="LightGray"
                 FontFamily="Montserrat"
                 BorderThickness="1"
                 BorderBrush="{StaticResource Verde}">
        </ListBox>
        <ListBox x:Name="lista2"
                 Width="275"
                 Height="250"
                 Background="Transparent"
                 Foreground="LightGray"
                 FontFamily="Montserrat"
                 BorderThickness="1"
                 BorderBrush="{StaticResource Verde}">
        </ListBox>
    </StackPanel>
</Page>
